voto : literal
inicio

Escreval("Digite seu voto")
leia(voto)

se voto = "20" entao
  Escreval ("Você votou em Lucas Trindade")
senao
se voto = "30" entao
  Escreval ("Você votou em Homem Aranha")
senao
se voto = "40" entao
  Escreval ("Você votou no Thor")
senao
  Escreval ("Voto nulo.")
fimse
fimse
fimse

fimalgoritmo


